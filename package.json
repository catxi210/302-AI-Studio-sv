    "quality": "npm run check && npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "generate:ipc": "tsx scripts/generate-ipc.ts",
    "postinstall": "pre-commit install --install-hooks --hook-type commit-msg"
